@import 'variables.styl'

.row_reporting > .col-md-4 {
  padding: 0 !important;
}

.row_reporting > .col-md-4:first-child {
  padding-left: 5px !important;
}

.row_reporting > .col-md-4:last-child {
  padding-right: 5px !important;
}
.row_reporting .label{
  font-size: 95%;
}

.row-label>div{
  padding:0;
}
.row-label>div:first-child{
  padding-left:2px;
}
.row-label>div:last-child{
  padding-left:5px;
}

.progress-bar span{
  font-weight: bold !important;
  font-size: 115% !important;
}
